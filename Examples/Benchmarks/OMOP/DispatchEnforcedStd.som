DispatchEnforcedStd = Benchmark (
    | domain |

    benchmark = (
        Mirror evaluate: [
            1 to: 20000 do: [ :i | self method: i ]
        ] enforcedIn: domain.
    )
    
    method: argument = ( ^argument )
    
    initialize = (
        domain = Mirror currentDomain
    )
    
    ----
    
    new = (
        ^ super new initialize
    )
)
